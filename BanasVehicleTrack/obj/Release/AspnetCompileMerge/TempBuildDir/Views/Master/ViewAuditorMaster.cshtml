@model BanasVehicleTrackViewModel.Master.AuditorMasterViewModel@{ ViewBag.Title = "Auditor Master";    Layout = "~/Views/Shared/_Layout.cshtml";}<div class="page-wrapper">    <div class="page-header">        <div class="row align-items-end">            <div class="col-lg-12">                <div class="float-left">                    <h3><i class="ti-control-forward ForwardIcon"> </i>Auditor Master</h3>                </div>                <div class="float-right">                    <a class="btn btn-outline-primary btn-round" href="@Url.Action("AddAuditorMaster","Master")">                        <i class="fa fa-plus mx-2"></i>Add Auditor                    </a>                </div>            </div>        </div>    </div>    <div class="card">        <div class="card-block">            <div class="dt-responsive table-responsive">                <table id="base-style" class="table table-striped  table-hover table-bordered nowrap">                    <thead>                        <tr>                            <th>                                @Html.DisplayName("Action")                            </th>                            <th>                                @Html.DisplayName("Auditor Code")                            </th>                            <th>                                @Html.DisplayName("Password")                                                           </th>                            <th>                                @Html.DisplayName("Auditor Name")                            </th>                            <th>                                @Html.DisplayName("Auditor Signature")                            </th>                            <th>                                @Html.DisplayName("IsActive")                            </th>                            <th>                                @Html.DisplayName("Create Date")                            </th>                            <th>                                @Html.DisplayName("Create User")                            </th>                        </tr>                    </thead>                    <tbody>                        @foreach (var item in Model.AuditorMasterList)                        {                        <tr>                            <td>                                @if (ViewBag.UpdateRight == 1)                                {                        @Html.ActionLink(" ", "EditAuditorMaster", new { id = item.AuditorId }, new { @class = " btn editbtn fa fa-pencil-square-o", title = "Edit" })}                            </td>                            <td>                                @Html.DisplayFor(modelItem => item.AuditorCode)                            </td>                            <td>                                @*@Html.DisplayFor(modelItem => item.Password)*@                                **********                            </td>                            <td>                                @Html.DisplayFor(modelItem => item.AuditorName)                            </td>                            <td>                                <img src="../@item.AuditorSignature" alt="Auditor Signature" width="150" height="50">                            </td>                            <td>                                <label class="toggle">                                    <input class="toggle-checkbox" type="checkbox" onclick='ActiveDeactiveStatusChange(this,"@item.AuditorId","AuditorMaster")' @(item.IsActive == true ? "checked" : "")>                                    <span class="toggle-switch"></span>                                </label>                            </td>                            <td> @Html.DisplayFor(modelItem => item.cdate)</td>                            <td> @Html.DisplayFor(modelItem => item.CreateUser)</td>                        </tr>}                    </tbody>                </table>            </div>        </div>    </div></div>